{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Flask",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/prime_numbers.py",
      "args": [
        "run",
        "--no-debugger",
        "--no-reload"
      ],
      "jinja": true,
      "env": {
        "FLASK_APP": "prime_numbers.py",
        "FLASK_ENV": "development"
      },
      "console": "integratedTerminal"
    }
  ]
}